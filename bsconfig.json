{
  "name": "rescript-react-realworld-example-app",
  "namespace": true,
  "suffix": ".bs.js",
  "reason": { "react-jsx": 3 },
  "bsc-flags": [
    "-bs-super-errors",
    "-bs-no-version-header"
  ],
  "sources": {
    "dir": "src",
    "subdirs": [
      { "dir": "page", "subdirs": [{ "dir": "Article" }] },
      { "dir": "component" },
      { "dir": "shared" }
    ]
  },
  "package-specs": [
    {
      "module": "es6",
      "in-source": true
    }
  ],
  "bs-dependencies": [
    "@rescript/react",
    "@ryyppy/rescript-promise",
    "bs-fetch",
    "rescript-webapi"
  ],
  "bs-dev-dependencies": []
}
